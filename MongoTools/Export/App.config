<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="sourceServer"        value="YOURSERVERADDRESS:YOURPORT"/>
    <add key="authDatabaseName"    value="admin"/>
    <add key="sourceDatabaseName"  value="YOUR_SOURCE_DATABASE"/>
    <add key="collection"          value="YOUR_COLLECTION"/>
    <add key="sourceUsername"      value="LOGIN"/>
    <add key="sourcePassword"      value="PASSWORD"/>
    <add key="mongoQuery"          value=""/>
    <add key="outputFile"          value="YOURPATH\out.csv"/>
    <add key="format"              value="csv"/>
    <add key="layoutFile"          value="YOURPATH"/>
    <add key="limit"               value="-1"/>
    <add key="addHeader"           value="true"/>
  </appSettings>
</configuration>