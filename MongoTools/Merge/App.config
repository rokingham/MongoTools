<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="authDatabaseNameSource" value="admin"/>
    <add key="authDatabaseNameTarget" value="admin"/>
    <add key="sourceServer"           value="SOURCESERVERADDRESS:YOURSOURCEPORT"/>
    <add key="sourceUsername"         value="SOURCE_DB_USERNAME"/>
    <add key="sourcePassword"         value="SOURCE_DB_PASS"/>
    <add key="targetServer"           value="TARGETSERVERADDRESS:YOURTARGETPORT"/>
    <add key="targetUsername"         value="TARGET_DB_USERNAME"/>
    <add key="targetPassword"         value="TARGET_DB_PASSWORD"/>
    <add key="sourceDatabaseName"     value="SOURCE_DATABASE_NAME"/>
    <add key="targetDatabaseName"     value="TARGET_DATABASE_NAME"/>
    <add key="insertBatchSize"        value="100"/>
  </appSettings>
</configuration>